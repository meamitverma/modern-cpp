#include <iostream>
#include <string>

class RBIBank {
public:
    virtual std::string getBankName() const = 0;
    virtual void withdrawMoney(double amount) = 0;
};

class SBIBank : public RBIBank {
private:
    double balance{10000.0};

public:
    std::string getBankName() const override {
        return "State Bank of India (SBI)";
    }
-
    void withdrawMoney(double amount) override {
        if (amount > balance) {
            std::cout << "Insufficient balance!\n";
        } else {
            balance -= amount;
            std::cout << "Withdrawal successful. New balance: " << balance << std::endl;
        }
    }
};

int main() {
    SBIBank sbiBank;
    std::cout << "Bank Name: " << sbiBank.getBankName() << std::endl;

    sbiBank.withdrawMoney(5000.0);
    sbiBank.withdrawMoney(6000.0); // Should print "Insufficient balance!"

    return 0;
}
